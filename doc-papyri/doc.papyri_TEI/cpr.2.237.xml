</phr>
<phr>
<w lemma="ⲅ" type="NUM">
ⲅ⳿
</w>
</phr>
<lb n="5"/>
<phr>
<w lemma="ⲛⲧⲉ" type="PREP">
ⲧⲉ
</w>
</phr>
<phr>
<w lemma="ⲥⲓⲙⲱⲛ" type="NPROP">
ⲥⲓⲙⲱⲛ
</w>
</phr>
<phr>
<w lemma="ⲡ" type="ART">
ⲡ
</w>
<w lemma="ϣⲏⲣⲉ" type="NPROP">
ϣⲉ
</w>
</phr>
<phr>
<w lemma="ⲛ" type="PREP">
ⲛ
</w>
<w lemma="ⲁⲡⲁ" type="N">
ⲁⲡⲁ
</w>
</phr>
<phr>
<w lemma="ⲓⲟⲩⲗⲉ" type="NPROP">
ⲓⲟⲩⲗⲉ
</w>
</phr>
</s>
<s style="Give it to Shenoute, (at) the camelherd, who takes it for me. ">
<phr>
<w lemma="ϯ" type="V">
ⲧⲁⲁ
</w>
<w lemma="ⲛⲧⲟϥ" type="PPERO">
ϥ
</w>
</phr>
<phr>
<w lemma="ϣⲉⲛⲟⲩⲧⲉ" type="NPROP">
ϣⲉⲛⲟⲩⲧⲉ
</w>
</phr>
<lb n="6"/>
<phr>
<w lemma="ⲡ" type="ART">
ⲡ
</w>
<w lemma="ⲙⲁ" type="N">
ⲙⲁ
</w>
</phr>
<phr>
<w lemma="ⲛ" type="PREP">
ⲛ
</w>
<w lemma="ϭⲁⲙⲟⲩⲗ" type="N">
ϭⲁⲙⲟⲩⲗ
</w>
</phr>
<phr>
<w lemma="ⲉⲣⲉ" type="CCIRC">
ⲉ
</w>
<w lemma="ⲛⲧⲟϥ" type="PPERS">
ϥ
</w>
<w lemma="ϭⲱⲡⲉ" type="V">
ϭⲱⲡⲉ
</w>
</phr>
<phr>
<w lemma="ⲛ" type="PREP">
ⲙⲱ
</w>
<w lemma="ⲛⲧⲟϥ" type="PPERO">
ϥ
</w>
</phr>
<phr>
<w lemma="ⲉ" type="PREP">
ⲉⲣⲱ
</w>
<w lemma="ⲁⲛⲟⲕ" type="PPERO">
ⲓ
</w>
</phr>
</s>
<s style="(Be) safe in God.">
<phr>
<w lemma="ⲟⲩϫⲁⲓ" type="V">
ⲟⲩϫⲁⲓ
</w>
</phr>
<phr>
<w lemma="ϩⲛ" type="PREP">
ϩⲙ
</w>
<w lemma="ⲡ" type="ART">
ⲡ
</w>
<w lemma="ϫⲟⲉⲓⲥ" type="N">
ϫⲟⲉⲓⲥ
</w>
</phr>
</s>
</p>
<p>
<pb/>
<lb n="1"/>
<s style="Give it to my beloved brother Chael Iohannes. ">
<phr>
<w lemma="ϯ" type="V">
ⲧⲁⲁ
</w>
<w lemma="ⲛⲧⲟⲥ" type="PPERO">
ⲥ
</w>
</phr>
<phr>
<w lemma="ⲡⲁ" type="PPOS">
ⲡⲁ
</w>
<w lemma="ⲙⲉⲣⲓⲧ" type="N">
ⲙⲉⲣⲓⲧ
</w>
</phr>
<phr>
<w lemma="ⲛ" type="PREP">
ⲛ
</w>
<w lemma="ⲥⲟⲛ" type="N">
ⲥⲟⲛ
</w>
</phr>
<phr>
<w lemma="ⲭⲁⲏⲗ" type="NPROP">
ⲭ
ⲁⲏⲗ
</w>
</phr>
<phr>
<w lemma="ⲓⲱⲁⲛⲛⲏⲥ" type="NPROP">
ⲓⲱⲁⲛⲛⲏⲥ
</w>
</phr>
</s>
<s style="(signed) by Zaleh">
<phr>
<w lemma="ϩⲓⲧⲛ" type="PREP">
ϩⲓⲧⲛ
</w>
<w lemma="ⲍⲁⲗⲉϩ" type="NPROP">
ⲍⲁⲗⲉϩ
</w>
</phr>
