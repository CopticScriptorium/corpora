<?xml version='1.0' encoding='UTF-8'?><!DOCTYPE paula SYSTEM "paula_feat.dtd">
<paula version="1.1">
	<header paula_id="scriptorium.AP.143.n310.gabriel.mark_translation"/>
	<featList xmlns:xlink="http://www.w3.org/1999/xlink" type="translation" xml:base="scriptorium.AP.143.n310.gabriel.mark.xml">
		<feat xlink:href="#sSpan8" value="The devil changed into an image of an angel of the light."/>
		<feat xlink:href="#sSpan56" value="He appeared to one of the brothers and said to him,"/>
		<feat xlink:href="#sSpan103" value="“I am Gabriel, who was sent to you”"/>
		<feat xlink:href="#sSpan140" value="But he said to him,"/>
		<feat xlink:href="#sSpan161" value="“Look, you were sent to another of the brothers,"/>
		<feat xlink:href="#sSpan203" value="for I am not worthy.”"/>
		<feat xlink:href="#sSpan230" value="And he immediately disappeared."/></featList></paula>